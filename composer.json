{
  "autoload": {
    "psr-4": {
      "Src\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "symfony/http-foundation": "^7.3",
    "doctrine/orm": "^3.5",
    "twig/twig": "^3.21",
    "doctrine/dbal": "^4.3",
    "symfony/cache": "^7.3",
    "phpmailer/phpmailer": "^6.10",
    "respect/validation": "^2.4",
    "vlucas/phpdotenv": "^5.6",
    "eftec/bladeone": "^4.19",
    "spomky-labs/otphp": "^11.3"
  },
  "require-dev": {
    "vimeo/psalm": "^6.13",
    "phpunit/phpunit": "^12.3",
    "symfony/var-dumper": "^7.3",
    "weirdan/doctrine-psalm-plugin": "^2.10"
  },
  "scripts": {
    "doctrine-create": "docker exec -it magiciptv-web-1 php bin/doctrine orm:schema-tool:create",
    "doctrine-update": "docker exec -it magiciptv-web-1 php bin/doctrine orm:schema-tool:update --force"
  }
}
